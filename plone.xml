---
layout: feed
---
{% for post in site.categories.plone %}
<entry>
    <title>{{ post.title }}</title>
    <link rel="alternate" type="text/html"
          href="http://www.speedbreeze.com{{ post.url }}" />
    <id>http://www.speedbreeze.com{{ post.url }}</id>
    <summary>
        {{ post.content }}
    </summary>
    <updated>{{ post.date | date: '%Y-%m-%dT%H:%M:%SZ' }}</updated>
</entry>
{% endfor %}
